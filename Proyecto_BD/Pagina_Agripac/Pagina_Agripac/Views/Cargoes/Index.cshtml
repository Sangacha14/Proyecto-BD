@model IEnumerable<Pagina_Agripac.Cargo>

@{
    ViewBag.Title = "Index";
}

<h2>Cargos de la Empresa</h2>

<p>
    @Html.ActionLink("Crear Cargo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cargo1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cargo1)
        </td>
        <td>
            @Html.ActionLink("✏", "Edit", new { id=item.id_cargo }) |
            @Html.ActionLink("👀", "Details", new { id=item.id_cargo }) |
            @Html.ActionLink("❌", "Delete", new { id=item.id_cargo })
        </td>
    </tr>
}

</table>
